name: 增强流媒体检测
desc: 支持 Netflix/Disney+/ChatGPT/YouTube/TikTok。在第三方服务中独立显示每个服务的状态。
author: xiaomingyve1
update-url: https://github.com/xiaomingyve1/stash
enable: true

# 【关键】：这里定义了5个独立的面板项
panel-items:
  - script: check-netflix
  - script: check-disney
  - script: check-youtube
  - script: check-chatgpt
  - script: check-tiktok

script-providers:
  # 1. Netflix
  check-netflix:
    url: https://raw.githubusercontent.com/xiaomingyve1/stash/main/script/media-detection.js
    interval: 300
    schedule: [manual, network-change]
    no-cache: true
    argument: "Netflix"

  # 2. Disney+
  check-disney:
    url: https://raw.githubusercontent.com/xiaomingyve1/stash/main/script/media-detection.js
    interval: 300
    schedule: [manual, network-change]
    no-cache: true
    argument: "Disney"

  # 3. YouTube
  check-youtube:
    url: https://raw.githubusercontent.com/xiaomingyve1/stash/main/script/media-detection.js
    interval: 300
    schedule: [manual, network-change]
    no-cache: true
    argument: "YouTube"

  # 4. ChatGPT
  check-chatgpt:
    url: https://raw.githubusercontent.com/xiaomingyve1/stash/main/script/media-detection.js
    interval: 300
    schedule: [manual, network-change]
    no-cache: true
    argument: "ChatGPT"

  # 5. TikTok
  check-tiktok:
    url: https://raw.githubusercontent.com/xiaomingyve1/stash/main/script/media-detection.js
    interval: 300
    schedule: [manual, network-change]
    no-cache: true
    argument: "TikTok"
