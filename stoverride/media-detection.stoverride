name: 增强流媒体检测
desc: 支持 Netflix/Disney+/ChatGPT/YouTube/TikTok。
author: xiaomingyve1
update-url: https://github.com/xiaomingyve1/stash
enable: true

panel-items:
  - script: liumeitijiance

script-providers:
  liumeitijiance:
    url: https://raw.githubusercontent.com/xiaomingyve1/stash/main/script/media-detection.js
    no-cache: true
    schedule:
      - cron: "* * * * *"
      - interval: 300
      - network-change
      - manual
    policy: no-cache
